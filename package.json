{
    "scripts": {
        "dev-server": "USERNAME=\"${USER}\" webpack-dev-server --mode development --open --hot",
        "lint": "eslint . --ext .ts,.tsx",
        "lint-fix": "eslint --fix",
        "prettier": "prettier --check \"{src,test,dtslint,example}/**/*.{ts,tsx}\"",
        "prettier-fix": "prettier --write \"./{src,test,dtslint,example}/**/*.ts\"",
        "pretest": "yarn lint && yarn prettier",
        "test": "yarn test-unit",
        "test-unit": "jest",
        "test-type": "dtslint dtslint",
        "prebuild": "rm -rf lib && mkdir lib",
        "build": "tsc",
        "postbuild": "prettier --write \"./lib/**/*.ts\"",
        "prepublish": "yarn build"
    },
